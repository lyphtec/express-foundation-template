  "jspm": {
    "directories": {
      "baseURL": "client",
      "lib": "client/js",
      "packages": "client/lib"
    },
    "dependencies": {
      "body-parser": "^1.10.1",
      "bower-foundation": "github:zurb/bower-foundation@^5.5.1",
      "errorhandler": "^1.3.2",
      "express": "^4.11.0",
      "fastclick": "npm:fastclick@^1.0.6",
      "foundation": "^5.5.1",
      "jquery": "^2.1.3",
      "lodash": "^2.4.1",
      "markdown-serve": "^0.3.2",
      "method-override": "^2.3.1",
      "modernizr": "^2.8.3",
      "moment": "^2.9.0",
      "morgan": "^1.5.1",
      "multer": "^0.1.7",
      "serve-favicon": "^2.2.0",
      "vash": "^0.7.12-1",
      "winston": "^0.8.3"
    }
  },
  "paths": {
    "source": {
      "css": [
        "./client/css/**/*.css",
        "!./client/css/**/*.min.css"
      ],
      "sass": [
        "./client/scss",
        "./client/lib/github/zurb/bower-foundation@5.5.1/scss"
      ],
      "sassMain": "./client/scss/app.scss",
      "js": [
        "./client/js/**/*.js",
        "!./client/js/**/*.min.js"
      ]
    },
    "dest": {
      "css": "./client/css",
      "cssMain": "app.min.css",
      "js": "./client/js",
      "jsMain": "app.min.js"
    }
  }
}
